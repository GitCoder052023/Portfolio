{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "Server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "Public/CSS/tailwind.css",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Public/CSS"
        }
      },
      {
        "src": "Public/JS/script.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Public/JS"
        }
      },
      {
        "src": "Public/Templates/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/Server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/app.js"
      }
    ]
  }