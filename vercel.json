{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "Server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "Public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Server/server.js"
    },
    {
      "src": "/Templates/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/app.js"
    }
  ]
}